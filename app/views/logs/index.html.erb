<h1>Logs</h1>
<div class="container">

	<div class="row col-lg-12 form-group">
		<button><%= link_to 'Back', root_path %></button>
	</div>

	<div class="row col-lg-12 form-group">

		<table class="table table-striped table-bordered">

			<tr>
				<th>username</th>
				<th>created_at</th>
				<th>ip</th>
			</tr>

			<% @logs.each do |log| %>
			<tr>
				<td> <%= log.username %> </td>
				<td> <%= log.created_at %> </td>
				<td> <%= log.ip %> </td>
			</tr>
			<% end %>

		</table>
	</div>
	
</div>